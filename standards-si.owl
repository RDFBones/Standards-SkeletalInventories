    <!-- http://w3id.org/rdfbones/ext/standards-si/1-complete -->

    <owl:NamedIndividual rdf:about="http://w3id.org/rdfbones/ext/standards-si/1-complete">
        <rdf:type rdf:resource="http://w3id.org/rdfbones/ext/standards-si/Representation4StatesLabel"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An element is defined as complete if it is present to at least 75%. The category &apos;complete&apos; is also expressed with the numeric code 1.</obo:IAO_0000115>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Buikstra and Ubelaker 1994, 6</obo:IAO_0000119>
        <obo:IAO_0000117 xml:lang="en">Felix Engel</obo:IAO_0000117>
        <rdfs:label xml:lang="en">Complete</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/rdfbones/ext/standards-si/2-partial -->

    <owl:NamedIndividual rdf:about="http://w3id.org/rdfbones/ext/standards-si/2-partial">
        <rdf:type rdf:resource="http://w3id.org/rdfbones/ext/standards-si/Representation4StatesLabel"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An element is defined as partially represented if it is present to at least 25%. The category &apos;partial&apos; is also expressed with the numeric code 2.</obo:IAO_0000115>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Buikstra and Ubelaker 1994, 6</obo:IAO_0000119>
        <obo:IAO_0000117 xml:lang="en">Felix Engel</obo:IAO_0000117>
        <rdfs:label xml:lang="en">Partial</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/rdfbones/ext/standards-si/3-poor -->

    <owl:NamedIndividual rdf:about="http://w3id.org/rdfbones/ext/standards-si/3-poor">
        <rdf:type rdf:resource="http://w3id.org/rdfbones/ext/standards-si/Representation4StatesLabel"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An element is defined as poorly represented if it is present to less than 25%. The category &apos;poor&apos; is also expressed with the numeric code 3.</obo:IAO_0000115>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Buikstra and Ubelaker 1994, 6</obo:IAO_0000119>
        <obo:IAO_0000117 xml:lang="en">Felix Engel</obo:IAO_0000117>
        <rdfs:label xml:lang="en">Poor</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/rdfbones/ext/standards-si/missing -->

    <owl:NamedIndividual rdf:about="http://w3id.org/rdfbones/ext/standards-si/missing">
        <rdf:type rdf:resource="http://w3id.org/rdfbones/ext/standards-si/Representation4StatesLabel"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An element is defined as missing if it is not represented by any material. In the recording scheme published by Buikstra and Ubelaker (1994, 6) the category &apos;missing&apos; is expressed by leaving data fields blank.</obo:IAO_0000115>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Buikstra and Ubelaker 1994, 6</obo:IAO_0000119>
        <obo:IAO_0000116 xml:lang="en">The option &apos;missing&apos; is represented by an explicit categorical label, here, to satisfy the general requirement in data management to distinguish between NULL (&apos;not available&apos;) and zero (&apos;not present&apos;).</obo:IAO_0000116>
        <obo:IAO_0000117 xml:lang="en">Felix Engel</obo:IAO_0000117>
        <rdfs:label xml:lang="en">Missing</rdfs:label>
    </owl:NamedIndividual>


    
</rdf:RDF>

