    <!-- http://w3id.org/rdfbones/ext/standards-si/RightSideFrontalBone -->

    <owl:Class rdf:about="http://w3id.org/rdfbones/ext/standards-si/RightSideFrontalBone">
        <rdfs:subClassOf rdf:resource="http://w3id.org/rdfbones/core#SegmentOfBoneOrgan"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/sig/ont/fma/regional_part_of"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://purl.org/sig/ont/fma/fma52734"/> <!-- 'Frontal bone' -->
                            <rdf:Description rdf:about="http://purl.org/sig/ont/fma/fma0327999"/> <!-- 'Right fetal frontal bone' -->
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A region of interest (rdfbones:AnatomicalRegionOfInterest) describing the part of a frontal bone that is right of the saggital anatomical plane.</obo:IAO_0000115>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Felix Engel</obo:IAO_0000119>
        <obo:IAO_0000116 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This anatomical region of interest describes the right side of any frontal bone, irrespectively of it being separated by a frontal suture or not. It can, therefore, refer to a right fetal frontal bone (class fma:0327999) or to the left side of a fused frontal bone (class fma:52734). As a consequence, instances of this class can describe skeletal material from both adult and subadult individuals.</obo:IAO_0000116>
        <obo:IAO_0000116 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A value for the data property rdfbones:bonePortion cannot be defined on class level as it&apos;s instances can be regional parts of either a right fetal frontal bone (bone portion = 1) or of a mature frontal bone (bone portion = 0.5).</obo:IAO_0000116>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Felix Engel</obo:IAO_0000117>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Right side of frontal bone</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/rdfbones/ext/standards-si/SectionCranialBonesAndJointSurfaces -->

    <owl:Class rdf:about="http://w3id.org/rdfbones/ext/standards-si/SectionCranialBonesAndJointSurfaces">
        <rdfs:subClassOf rdf:resource="http://w3id.org/rdfbones/core#SkeletalInventorySection"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000051"/> <!-- 'has part' -->
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://w3id.org/rdfbones/ext/standards-si/Representation4States"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/IAO_0000136"/> <!-- 'is about' -->
                                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                                <owl:onClass rdf:resource="http://w3id.org/rdfbones/core#EntireBoneOrgan_fma52789"/> <!-- Entire left parietal bone -->
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000051"/> <!-- 'has part' -->
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://w3id.org/rdfbones/ext/standards-si/Representation4States"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/IAO_0000136"/> <!-- 'is about' -->
                                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                                <owl:onClass rdf:resource="http://w3id.org/rdfbones/core#EntireBoneOrgan_fma52788"/> <!-- Entire right parietal bone -->
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section of the Inventory for Complete Skeletons from Buikstra and Ubelaker (1994) for the assessment of cranial elements.</obo:IAO_0000115>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Buikstra and Ubelaker 1994, chapter 2, attachment 1</obo:IAO_0000119>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Felix Engel</obo:IAO_0000117>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cranial bones and joint surfaces</rdfs:label>
    </owl:Class>
    


